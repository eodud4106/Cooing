<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cooing.www.jinsu.dao.SearchMapper">

	<insert id="insertHashTag" parameterType="tag">
	insert into tag (tag_num,tag_albumnum,tag_content)
		values (teg_seq.nextval,#{tag_albumnum},#{tag_content})
	</insert>
	
	<select id="selectHashTag" resultType="tag" parameterType="String">
		select * from tag where tag_content = #{search}
	</select>
	
	
</mapper>
